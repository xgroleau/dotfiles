# Note that oh-my-zsh already provides a lot of good aliases

# helpers
alias grep="grep --color=auto"
alias df="df -h"
alias du="du -h"

# nvim
alias v="nvim"

# tmux aliases
alias ta="tmux attach"
alias tls="tmux ls"
alias tat="tmux attach -t"
alias tns="tmux new-session -s"

# emacs aliases
alias em="emacsclient -nw -a 'emacs --daemon'"
alias emg="emacsclient -c -a 'emacs --daemon'"
